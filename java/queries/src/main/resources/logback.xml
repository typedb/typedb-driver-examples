<configuration debug="false">
    <root level="INFO"/>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%msg%n</pattern>
        </encoder>
    </appender>

    <logger name="ai.grakn.examples" level="INFO">
        <appender-ref ref="STDOUT"/>
    </logger>
</configuration>